#!/bin/bash 

orientdb_url="remote://$OPENSHIFT_ORIENTDB_DB_USERNAME:$OPENSHIFT_ORIENTDB_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_ORIENTDB_DB_PROXY_PORT/"

echo "OPENSHIFT_ORIENTDB_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_ORIENTDB_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_ORIENTDB_DB_USERNAME=$OPENSHIFT_ORIENTDB_DB_USERNAME"
echo "OPENSHIFT_ORIENTDB_DB_PASSWORD=$OPENSHIFT_ORIENTDB_DB_PASSWORD"
echo "OPENSHIFT_ORIENTDB_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_ORIENTDB_DB_PORT=$OPENSHIFT_ORIENTDB_DB_PROXY_PORT"
echo "OPENSHIFT_ORIENTDB_DB_URL=$orientdb_url"

# Version
echo "OPENSHIFT_ORIENTDB_VERSION=$OPENSHIFT_ORIENTDB_VERSION"